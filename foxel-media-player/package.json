{
  "name": "foxel-media-player-plus",
  "version": "1.0.3",
  "description": "A powerful media player plugin for Foxel with audio/video playback, playlist management, lyrics support, and advanced features",
  "main": "dist/plugin.js",
  "scripts": {
    "build": "esbuild src/index.tsx --bundle --format=iife --platform=browser --target=es2019 --jsx=automatic --minify --outfile=dist/plugin.js --define:process.env.NODE_ENV='\"production\"' --define:PLUGIN_VERSION='\"'$npm_package_version'\"' --define:PLUGIN_AUTHOR='\"'$npm_package_author'\"'",
    "dev": "esbuild src/index.tsx --bundle --format=iife --platform=browser --target=es2019 --jsx=automatic --outfile=dist/plugin.js --watch",
    "clean": "rm -rf dist",
    "validate": "node validate-plugin.js",
    "prebuild": "echo 'Building plugin...'"
  },
  "keywords": ["foxel", "plugin", "media", "player", "audio", "video", "music", "playlist", "lyrics"],
  "author": "Jason",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "esbuild": "^0.25.9",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
