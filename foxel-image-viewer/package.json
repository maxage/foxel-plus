{
  "name": "foxel-image-viewer",
  "version": "1.0.0",
  "description": "A simple image viewer plugin for Foxel",
  "main": "dist/plugin.js",
  "scripts": {
    "build": "esbuild src/index.tsx --bundle --format=iife --platform=browser --target=es2019 --jsx=automatic --minify --outfile=dist/plugin.js --define:process.env.NODE_ENV=\"production\"",
    "dev": "esbuild src/index.tsx --bundle --format=iife --platform=browser --target=es2019 --jsx=automatic --outfile=dist/plugin.js --watch",
    "clean": "rm -rf dist"
  },
  "keywords": ["foxel", "plugin", "image", "viewer"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "esbuild": "^0.19.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
