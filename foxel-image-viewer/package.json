{
  "name": "foxel-image-viewer",
  "version": "1.0.0",
  "description": "A simple image viewer plugin for Foxel",
  "main": "dist/plugin.js",
  "scripts": {
    "build": "esbuild src/index.tsx --bundle --format=iife --platform=browser --target=es2019 --jsx=automatic --outfile=dist/plugin.js --define:process.env.NODE_ENV='\"production\"'",
    "dev": "esbuild src/index.tsx --bundle --format=iife --platform=browser --target=es2019 --jsx=automatic --outfile=dist/plugin.js --watch",
    "clean": "rm -rf dist",
    "validate": "node validate-plugin.js",
    "prebuild": "npm run validate"
  },
  "keywords": ["foxel", "plugin", "image", "viewer"],
  "author": "Foxel Plus Team",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "esbuild": "^0.25.9",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
